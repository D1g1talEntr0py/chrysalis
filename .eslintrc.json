{
	"env": {
		"browser": true,
		"es2022": true,
		"node": true
	},
	"plugins": ["compat"],
	"extends": ["eslint:recommended", "plugin:compat/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": 2022,
		"sourceType": "module"
	},
	"rules": {
		"no-prototype-builtins": "off",
		"indent": [
			"error",
			"tab",
			{ "SwitchCase": 1 }
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		]
	}
}